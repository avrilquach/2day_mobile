doctype html
html
  include inc/head.jade
  body.m
    #wrapper_mobile
      include inc/header.jade
      section.breadcrumbs.pad10_0
        .container
          .row
            .col-lg-12
              ul
                li
                  a(href='#')
                    i.fas.fa-home
                    | Trang chủ
                li
                  a(href='#') Đời sống   

      .page_videos
        section.video_list
          .item
            .img
              img(src='img/2day/img3.jpg', alt='')
              a.popup-youtube(href='#')
                img(src='img/2day/icon.png', alt='')  
        section.video_list
          .swiper-container
            ul.swiper-wrapper
              -for(i=0;i<4;i++)
                li.swiper-slide
                  a.item(href='#').style2
                    .img
                      img(src='img/2day/img1.jpg', alt='')
                    .caption
                      h3 Sơn Tùng tại Sky Tour TP.HCM: Caó gì trao hết...
                      span Đời sống 
                        span.date | 13/08/2019
        section.video_list
          .row.justify-content-between.align-items-end
            h2._top-head._none_line Đời sống
            a.readmore(href='#') Xem thêm
          .item.style3
            .img
              img(src='img/2day/img3.jpg', alt='')
              a.popup-youtube(href='#')
                img(src='img/2day/icon.png', alt='')
            .caption.style3
              h3 Sơn Tùng tại Sky Tour TP.HCM: Caó gì trao hết...
              span Đời sống 
                span.date | 13/08/2019
          .row
            -for(i=0;i<4;i++)
              .item.style3
                .img
                  img(src='img/2day/img3.jpg', alt='')
                  a.popup-youtube(href='#')
                    img(src='img/2day/icon.png', alt='')
                .caption.style3
                  h3 Captin Maarel
                  span Đời sống 
                    span.date | 13/08/2019    
        section.video_list
          .row.justify-content-between.align-items-end
            h2._top-head._none_line Du lịch
            a.readmore(href='#') Xem thêm
          .item.style3
            .img
              img(src='img/2day/img3.jpg', alt='')
              a.popup-youtube(href='#')
                img(src='img/2day/icon.png', alt='')
            .caption.style3
              h3 Sơn Tùng tại Sky Tour TP.HCM: Caó gì trao hết...
              span Đời sống 
                span.date | 13/08/2019
          .row
            -for(i=0;i<4;i++)
              .item.style3
                .img
                  img(src='img/2day/img3.jpg', alt='')
                  a.popup-youtube(href='#')
                    img(src='img/2day/icon.png', alt='')
                .caption.style3
                  h3 Captin Maarel
                  span Du lịch
                    span.date | 13/08/2019  
      include inc/footer.jade
